{
  "name": "vscode-playwright-powerapps-snippets",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Deepak Kamboj",
  "description": "A Visual Studio Code extension which adds predefined useful code snippets for Playwright for PowerApps",
  "homepage": "https://github.com/deepakkamboj/vscode-playwright-powerapps-snippets",
  "keywords": [
    "Playwright"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/deepakkamboj/vscode-playwright-powerapps-snippets.git"
  },
  "bugs": "https://github.com/deepakkamboj/vscode-playwright-powerapps-snippets/issues",
  "displayName": "Playwright Snippets for PowerApps",
  "publisher": "deepakkamboj",
  "icon": "images/playwright-logo.png",
  "galleryBanner": {
    "color": "#2b3137",
    "theme": "dark"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "engines": {
    "vscode": "^1.53.0"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "vscode:publish": "vsce publish"
  },
  "devDependencies": {
    "jest": "^24.1.0"
  }
}

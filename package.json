{
  "name": "vscode-playwright-test-snippets",
  "version": "0.0.0-semantically-released",
  "license": "MIT",
  "author": "Deepak Kamboj",
  "publisher": "deepakkamboj",
  "displayName": "Playwright Code Snippets",
  "description": "A Visual Studio Code extension which adds predefined useful code snippets for Playwright",
  "homepage": "https://github.com/deepakkamboj/vscode-playwright-test-snippets",
  "keywords": [
    "Playwright"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/deepakkamboj/vscode-playwright-test-snippets.git"
  },
  "bugs": "https://github.com/deepakkamboj/vscode-playwright-test-snippets/issues",
  "icon": "images/playwright-logo.png",
  "galleryBanner": {
    "color": "#2b3137",
    "theme": "dark"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "snippets",
    "playwright",
    "playwright-test",
    "playwright-core"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "engines": {
    "vscode": "^1.53.0"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "vscode:publish": "vsce publish"
  },
  "devDependencies": {
    "jest": "^24.1.0",
    "prettier": "^2.0.5",
    "semantic-release": "^17.2.3",
    "semantic-release-vsce": "^3.0.1",
    "vsce": "^1.78.0"
  }
}
